msgstr "Sincronizar Publicaciones"

msgctxt "status_new"
msgid "Use new API"
msgstr "Utilizar nueva API"

msgctxt "status_new_summary"
msgid "Strongly recommended. Needs Android 4.0.3 or higher."
msgstr "Recomendado. Requiere Android 4.0.3 o superior."

msgctxt "contact_images"
msgid "Contact Images"
msgstr "Fotos de Perfil"

msgctxt "crop_square"
msgid "Crop to square"
msgstr "Recortar a un cuadrado"

