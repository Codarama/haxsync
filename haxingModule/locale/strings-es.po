msgid "Calendar Settings"
msgstr "Ajustes de Calendario"

msgctxt "calendar_summary"
msgid "Event Sync"
msgstr "Sincronización de Eventos"

msgctxt "sync_events"
msgid "Sync Events"
msgstr "Sincronizar Eventos"

msgctxt "sync_events_summary"
msgid "Will sync events you're attending to a separate Calendar if checked."
msgstr ""
"Si la opción está activa, se sincronizarán los eventos a los que "
"asistirás en un Calendario aparte."

msgctxt "contact_sync"
msgid "Contact Sync"
msgstr "Sincronización de Contactos"

msgctxt "phone_only"
msgid "Existing contacts only"
msgstr "Solo contactos existentes"

msgctxt "phone_only_summary"
msgid "Only sync contacts already on device."
msgstr "Solo se sincronizarán los contactos que ya están en el dispositivo."

